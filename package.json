  "type": "commonjs",
  "dependencies": {
    "@types/ejs": "^3.1.2",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "node": "^20.5.0",
    "path": "^0.12.7",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "typeorm": "^0.3.17",
    "winston": "^3.10.0"
  },
